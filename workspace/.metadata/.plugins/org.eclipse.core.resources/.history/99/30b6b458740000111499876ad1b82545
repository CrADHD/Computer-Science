import java.util.*;



public class SAT {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		double points = 0;			// int for eventual points
		double total = 15;			// int for eventual points/total
		double percent = 0;			// int for eventual percentage
		
		System.out.println("Would you like to play a game? type 1 " +
				"for yes. Of course no matter what you " +
				"WILL take this test..." +
				"" +
				"If you answer a Q correctly you get 1 point" +
				"If you answer incorrectly you get -0.25 point" +
				"If you skip you get 0 points");
		int WC = in.nextInt();		// WC = Who Cares / Does not matter
		
			int ans1 = Q1(WC);   // println requests
			int truth1 = in.nextInt();	   // user answer
			points = AnsCheck(ans1, truth1, points);
			
			int ans2 = Q1(WC);   // println requests
			int truth2 = in.nextInt();	   // user answer
			points = AnsCheck(ans2, truth2, points);
			System.out.println(points);
			break;
			
			int ans3 = Q1(WC);   // println requests
			int truth3 = in.nextInt();	   // user answer
			points = AnsCheck(ans3, truth3, points);
			
			
			int ans4 = Q1(WC);   // println requests
			int truth4 = in.nextInt();	   // user answer
			points = AnsCheck(ans4, truth4, points);
			
			
			int ans5 = Q1(WC);   // println requests
			int truth5 = in.nextInt();	   // user answer
			points = AnsCheck(ans5, truth5, points);
			
			
			int ans6 = Q1(WC);   // println requests
			int truth6 = in.nextInt();	   // user answer
			points = AnsCheck(ans6, truth6, points);

			
			int ans7 = Q1(WC);   // println requests
			int truth7 = in.nextInt();	   // user answer
			points = AnsCheck(ans7, truth7, points);
			
			
			int ans8 = Q1(WC);   // println requests
			int truth8 = in.nextInt();	   // user answer
			points = AnsCheck(ans8, truth8, points);
			
			
			int ans9 = Q1(WC);   // println requests
			int truth9 = in.nextInt();	   // user answer
			points = AnsCheck(ans9, truth9, points);
				
			
			int ans10 = Q1(WC);   // println requests
			int truth10 = in.nextInt();	   // user answer
			points = AnsCheck(ans10, truth10, points);
			
			
			int ans11 = Q1(WC);   // println requests
			int truth11 = in.nextInt();	   // user answer
			points = AnsCheck(ans11, truth11, points);
			
			
			int ans12 = Q1(WC);   // println requests
			int truth12 = in.nextInt();	   // user answer
			points = AnsCheck(ans12, truth12, points);
			
			
			int ans13 = Q1(WC);   // println requests
			int truth13 = in.nextInt();	   // user answer
			points = AnsCheck(ans13, truth13, points);
			
			
			int ans14 = Q1(WC);   // println requests
			int truth14 = in.nextInt();	   // user answer
			points = AnsCheck(ans14, truth14, points);
			
			
			int ans15 = Q1(WC);   // println requests
			int truth15 = in.nextInt();	   // user answer
			points = AnsCheck(ans15, truth15, points);
			
	}	
	
	
	public static double AnsCheck(int ans, int input, double points){
		
		if(ans==input){
			points = points + 1;
		}
	
		if(ans!=input){
			points = points - 0.25;
		}
		
		return points;
		
	}
		
	public static int Q1(int WC){
		
		System.out.println("Q1: Choose the word or set of words that," +		// Question 1
				"when inserted in the sentence, best " +
				"fits the meaning of the sentence as a whole." +
				"Ms. Fergusson’s main criticism of the artist’s " +
				"rendering of the ancient mammal’s physical " +
				"appearance is that, unsupported by even a ------- " +
				"of fossil evidence, the image is bound to be ------- .");
		System.out.println("1. modicum . . speculative");
		System.out.println("2. particle . . supplemented");
		System.out.println("3. perusal . . substantiated");
		System.out.println("4. fabrication . . obsolete");
		System.out.println("5. recapitulation . . exhausted");
		
		int ans = 1;
		return ans;
		
	}

	public static int Q2(int WC){				// Question 2
		
		System.out.println("Q2: A woman drove to work at an average speed " +
				"of  miles per hour and returned along the " +
				"same route at  miles per hour. If her total " +
				"traveling time was  hour, what was the total " +
				"number of miles in the round trip?");
		System.out.println("1. 30");
		System.out.println("2. 30 1/7");
		System.out.println("3. 34 2/7");
		System.out.println("4. 35");
		System.out.println("5. 40");
		
		int ans = 3;
		return ans;
		
	}
	
	public static int Q3(int WC){				// Question 3
		
		System.out.println("Q3: When, in 1864, a factory established by " +
				"Alfred Nobel to manufacture nitroglycerin " +
				"blew up, the scientist discovered that the " +
				"explosive was as ------- as it was powerful," +
				"------- to detonate without warning.");
		System.out.println("1. dormant . . ready");
		System.out.println("2. fickle . . unlikely");
		System.out.println("3. volatile . . liable");
		System.out.println("4. unprecedented . . intended");
		System.out.println("5. inactive . . designed");
		
		int ans = 3;
		return ans;
		
	}
	
	
	public static int Q4(int WC){				// Question 4
		
		System.out.println("Q4: Refuting the claim that the surest way " +
				"to reduce anger is to express it, the " +
				"author asserts that ------- anger can " +
				"actually increase its ------- .");
		System.out.println("1. denying . . impact");
		System.out.println("2. understanding . . importance");
		System.out.println("3. overcoming . . likelihood");
		System.out.println("4. venting . . intensity");
		System.out.println("5. voicing . . benefits");

		int ans = 4;
		return ans;
		
	}
	
	public static int Q5(int WC){				// Question 5
		
