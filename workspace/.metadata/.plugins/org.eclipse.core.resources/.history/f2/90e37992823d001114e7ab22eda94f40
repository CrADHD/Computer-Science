import java.io.*;
import java.util.*;
public class Temp {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		String last_name;
		Scanner in = new Scanner(System.in);
		System.out.println("Enter in new last name");
		last_name = in.nextLine();
		System.out.println(last_name);
		
	}

}
